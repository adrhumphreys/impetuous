{
    "name": "adrhumphreys/impetuous",
    "description": "Static caching made easy",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "Static Cache"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "guzzlehttp/guzzle": "^6.5",
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "AdrHumphreys\\Impetuous\\": "src/",
            "AdrHumphreys\\Impetuous\\Tests\\": "tests/"
        },
        "files": [
            "./includes/urlToPath.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
